<% include ../partials/header %>
<header>
    <nav>
        <ul>
            <li><a href="/storms/add">Add</a></li>
        </ul>
    </nav>    
</header>
<section>
    <h1>Storms</h1>
    <% storms.forEach(function(storm){ %>
        <div class="stormContainer">
            <p><%=storm.name%> <%= storm.category%></p><a href="/storms/<%=storm._id%>">Details</a>
        </div>
    <% });; %>
</section>
<% include ../partials/footer %>